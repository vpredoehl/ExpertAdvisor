do $code$
declare
	f text;
	qs text;
begin
	--FOR f in select matviewname from pg_matviews  where matviewname like '%fractal%'  order by 1
	FOR f in select tablename from pg_tables  where tablename like '%fractal%'  order by 1
	LOOP
		qs = format('drop table %I', f);
		--raise info 'executing: %', qs;
		begin
			execute  qs;
		exception when others then 
			raise notice '% %', SQLERRM, SQLSTATE;
		end;
	END LOOP;
end $code$
language 'plpgsql';

