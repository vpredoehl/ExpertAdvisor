do $code$
declare
	f text;
	qs text;
	oid int;
	isready bool;
begin
	FOR f in select indexname from pg_indexes  where tablename like 'rmp%'  order by tablename
	LOOP
		qs = format('reindex index %I', f);
		raise info 'executing: %', qs;
		begin
			execute  qs;
		exception when others then 
			raise notice '% %', SQLERRM, SQLSTATE;
		end;
	END LOOP;
end $code$
language 'plpgsql';
